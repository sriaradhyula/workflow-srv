    "extensions": [
        {
            "name": "oasf.agntcy.org/feature/runtime/manifest",
            "data": {
                "deployment": {
                    "agent_deps": [],
                    "deployment_options": [
                        {
                            "type": "source_code",
                            "name": "src",
                            "url": "some_url.git",
                            "framework_config": {
                                "framework_type": "langgraph",
                                "graph": "mock_graph",
                                "mock_agent": "tests.mock:mock_agent"
                            }
                        }
                    ],
                    "env_vars": []
                },
                "acp": {
                    "capabilities": {
                        "threads": false,
                        "interrupts": true,
                        "callbacks": true
                    },
                    "input": {
                        "type": "object",
                        "properties": {
                            "message": {
                                "type": "string"
                            }
                        }
                    },
                    "output": {
                        "type": "object",
                        "properties": {
                            "message": {
                                "type": "string"
                            }
                        }
                    },
                    "config": {
                        "type": "object",
